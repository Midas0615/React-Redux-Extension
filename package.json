{
  "name": "flyby",
  "version": "0.0.11",
  "description": "flyby chrome extension",
  "author": "Nisheeth Kashyap<nisheeth.k.kashyap@gmail.com>",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development API_URL=http://localhost:8080 webpack --watch",
    "start:lan": "NODE_ENV=development NETWORK=lan API_URL=https://happywallet.lan webpack --watch",
    "build": "NODE_ENV=production API_URL=https://api.happywallet.co:8000 webpack && zip -r -X happywallet.zip ./dist",
    "build:lan": "NODE_ENV=production NETWORK=lan API_URL=https://happywallet.lan webpack && zip -r -X happywallet-home.zip ./dist",
    "lint": "eslint . --ext .js --ignore-path .eslintignore --cache",
    "test": "NODE_ENV=test BABEL_ENV=test mocha --watch",
    "coverage": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "update": "rm -rf ./node_modules/injector/ && npm install file:../injector"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:nisheethkashyap/flyby.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "babel-runtime": "^6.11.6",
    "chart.js": "^2.3.0",
    "js-cookie": "^2.1.3",
    "material-ui": "^0.15.3",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "request": "^2.65.0",
    "request-promise-native": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-rewire": "^1.0.0-rc-5",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "ignore-styles": "^4.0.0",
    "isparta": "^4.0.0",
    "jsdom": "^9.4.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.0",
    "mocha-notifier-reporter": "^0.1.1",
    "node-sass": "^3.8.0",
    "redux-immutable-state-invariant": "^1.2.3",
    "sass-loader": "^4.0.0",
    "simulant": "^0.2.2",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "sinon-chrome": "^1.1.2",
    "style-loader": "^0.13.1",
    "stylelint-config-standard": "^12.0.0",
    "stylelint-webpack-plugin": "^0.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  }
}
